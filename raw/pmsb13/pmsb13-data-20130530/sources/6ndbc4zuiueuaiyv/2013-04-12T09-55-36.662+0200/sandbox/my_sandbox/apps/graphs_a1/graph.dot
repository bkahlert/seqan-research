digraph G {

/* Graph Attributes */
graph [rankdir = LR];

/* Node Attributes */
node [shape = rectangle, fillcolor = white, style = filled, fontname = "Times-Italic"];

/* Edge Attributes */
edge [fontname = "Times-Italic", arrowsize = 0.75, fontsize = 16];

/* Nodes */
0 [label = "0"];
1 [label = "1"];

/* Edges */
1 -> 0 [];

}
