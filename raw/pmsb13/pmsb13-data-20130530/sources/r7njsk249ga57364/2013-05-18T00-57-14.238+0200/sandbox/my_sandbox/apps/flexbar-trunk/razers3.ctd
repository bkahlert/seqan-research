			<ITEM name="unique" value="false" type="string" description="Output only unique best matches (-m 1 -dr 0 -pa)." restrictions="true,false"  />
			<ITEM name="trim-reads" value="" type="int" description="Trim reads to given length. Default: off." restrictions="14:"  />
			<ITEM name="output" value="" type="string" description="Mapping result filename. Default: &lt;READS FILE&gt;.razers." tags="output file" supported_formats="*razers,*eland,*fa,*fasta,*gff,*sam,*afg"  />
