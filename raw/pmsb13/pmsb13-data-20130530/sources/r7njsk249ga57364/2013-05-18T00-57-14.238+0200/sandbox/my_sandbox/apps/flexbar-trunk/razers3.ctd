			<ITEM name="param-dir" value="" type="string" description="Read user-computed parameter files in the directory &lt;DIR&gt; (swift)."  />
			<ITEM name="threshold" value="" type="int" description="Manually set minimum k-mer count threshold (swift)." restrictions="1:"  />
			<ITEM name="taboo-length" value="1" type="int" description="Set taboo length (swift)." restrictions="1:"  />
			<ITEM name="shape" value="" type="string" description="Manually set k-mer shape."  />
			<ITEM name="overabundance-cut" value="1" type="int" description="Set k-mer overabundance cut ratio." restrictions="0:1"  />
			<ITEM name="repeat-length" value="1000" type="int" description="Skip simple-repeats of length &lt;NUM&gt;." restrictions="1:"  />
			<ITEM name="load-factor" value="1.6" type="double" description="Set the load factor for the open addressing k-mer index." restrictions="1:"  />
			<ITEM name="match-N" value="false" type="string" description="N matches all other characters. Default: N matches nothing." restrictions="true,false"  />
			<ITEM name="error-distr" value="" type="string" description="Write error distribution to FILE."  />
			<ITEM name="mismatch-file" value="" type="string" description="Write mismatch patterns to FILE."  />
			<ITEM name="compact-mult" value="2.2" type="double" description="Multiply compaction treshold by this value after reaching and compacting." restrictions="0:"  />
