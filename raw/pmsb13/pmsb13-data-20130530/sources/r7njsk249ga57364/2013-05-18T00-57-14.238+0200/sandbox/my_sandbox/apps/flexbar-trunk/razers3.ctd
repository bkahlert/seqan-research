			<ITEM name="parallel-verification-max-package-count" value="100" type="int" description="Largest number of packages to create for verification per thread-1." restrictions="1:"  />
			<ITEM name="available-matches-memory-size" value="0" type="int" description="Bytes of main memory available for storing matches." restrictions="-1:"  />
