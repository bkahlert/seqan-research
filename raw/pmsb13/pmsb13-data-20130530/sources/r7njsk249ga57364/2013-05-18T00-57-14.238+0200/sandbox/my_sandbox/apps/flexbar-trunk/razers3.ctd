			<ITEM name="percent-identity" value="95" type="double" description="Percent identity threshold." restrictions="50:100"  />
			<ITEM name="recognition-rate" value="99" type="double" description="Percent recognition rate." restrictions="80:100"  />
			<ITEM name="no-gaps" value="false" type="string" description="Allow only mismatches, no indels. Default: allow both." restrictions="true,false"  />
			<ITEM name="forward" value="false" type="string" description="Map reads only to forward strands." restrictions="true,false"  />
			<ITEM name="reverse" value="false" type="string" description="Map reads only to reverse strands." restrictions="true,false"  />
