			<ITEM name="distance-range" value="" type="int" description="Only consider matches with at most NUM more errors compared to the best. Default: output all."  />
			<ITEM name="genome-naming" value="0" type="int" description="Select how genomes are named (see Naming section below)." restrictions="0:1"  />
			<ITEM name="read-naming" value="0" type="int" description="Select how reads are named (see Naming section below)." restrictions="0:3"  />
