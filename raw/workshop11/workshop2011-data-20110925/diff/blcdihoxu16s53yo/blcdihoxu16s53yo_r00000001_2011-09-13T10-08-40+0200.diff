+    bool showHelp;
+    bool showVersion;
+    int i;
+    String<CharString> texts;
+    
+    Options()
+    {
+        // Set defaults.
+        showHelp = false;
+        showVersion = false;
+        i = 0;
+    }
+};
+
+// ============================================================================
+// Metafunctions
